{
  "name": "charlottestuby.com",
  "description": "Charlotte stuby's online portfolio",
  "version": "2.0.0",
  "author": "Lucas Vogel <contact@vogelino.com> (https://vogelino.com)",
  "main": "n/a",
  "scripts": {
    "dev": "next",
    "test": "jest --passWithNoTests",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "lint-fix": "npm run lint -- --fix",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "keywords": [
    "gatsby",
    "netlify",
    "netlify cms",
    "portfolio",
    "artist",
    "swiss",
    "brussels",
    "textile"
  ],
  "homepage": "https://charlottestuby.com",
  "license": "MIT",
  "dependencies": {
    "frontmatter-markdown-loader": "3.6.3",
    "gray-matter": "4.0.3",
    "gsap": "3.7.1",
    "netlify-cms-app": "2.14.32",
    "netlify-cms-media-library-cloudinary": "1.3.10",
    "netlify-cms-media-library-uploadcare": "0.5.10",
    "nuka-carousel": "4.7.9",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "5.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/preset-react": "7.14.5",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@types/node": "14.17.6",
    "@types/react": "17.0.15",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.2.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "next": "10.2.3",
    "prettier": "2.2.1",
    "typescript": "4.3.5"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged && npm test -- -o --coverage --watchAll=false --passWithNoTests"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
